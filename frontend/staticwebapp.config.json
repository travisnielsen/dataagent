{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/images/*", "/*.ico", "/*.png", "/*.svg", "/*.json"]
  },
  "routes": [
    {
      "route": "/api/*",
      "statusCode": 404
    }
  ],
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "frame-ancestors 'none'"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
